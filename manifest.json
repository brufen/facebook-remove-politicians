{
  "name": "Hide Serbian Political Facebook Posts",
  "manifest_version" : 2,
  "version": "1.0",
  "description": "This extension will remove most Serbian political posts from your news feed.",
  "icons": { "128": "icon_enable.png" },
  "background": {
    "scripts": ["run.js"]
  },
  "permissions": ["http://*.facebook.com/", "https://*.facebook.com/"],
  "browser_action": {
    "default_title": "Hide Political Posts",
    "default_icon": "icon_disable.png",
  "author": "Marija G"
  }
}
